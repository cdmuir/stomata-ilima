	file = {Buckley and Diaz-Espejo - 2015 - Partitioning changes in photosynthetic rate into c.pdf:/Users/cdmuir/Zotero/storage/3ENBUSHE/Buckley and Diaz-Espejo - 2015 - Partitioning changes in photosynthetic rate into c.pdf:application/pdf},
}

@article{franks_plasticity_2009,
	title = {Plasticity in maximum stomatal conductance constrained by negative correlation between stomatal size and density: an analysis using \textit{{Eucalyptus} globulus}},
