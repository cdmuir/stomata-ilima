	pages = {2235--2248},
	file = {Evans et al. - 2009 - Resistances along the CO2 diffusion pathway inside.pdf:/Users/cdmuir/Zotero/storage/T2F6HUX4/Evans et al. - 2009 - Resistances along the CO2 diffusion pathway inside.pdf:application/pdf},
}

